{
  "restartable": "rs",
  "ignore": [".git", "node_modules/", "dist/", "coverage/"],
  "watch": ["src/"],
  "ext": ".ts",
  "execMap": {
    "ts": "node -r ts-node/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "exec999": "ts-node ./packages/cli/src/HDTInfo.ts",
  "exec9999": "npm run build",
  "ext": "js,json,ts"
}
